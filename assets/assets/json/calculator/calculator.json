{
  "data": {
    "title": "Hemodynamics Calculator",
    "calculators": [
      "Fick’s Cardiac Output and Cardiac Index",
      "Calculate Shock Risk Score",
      "CPO & PAPI (MCS Escalation)",
      "SV (ml/m2/beat) & CO (L/min)",
      "MAP (mmHg)",
      "SVR (dyne*sec*cm^-5)",
      "PVR (dyne*sec*cm^-5)",
      "Transpulmonary Gradient",
      "Absolute Weight-based Drip Rate"
    ],
    "options": {
      "Fick’s Cardiac Output and Cardiac Index": {
        "title": "Fick’s Cardiac Output and Cardiac Index",
        "parameters": [
          {
            "title": "Patient Weight",
            "unit": "kg"
          },
          {
            "title": "Height",
            "unit": "cm"
          },
          {
            "title": "SaO2",
            "unit": "%"
          },
          {
            "title": "SvO2",
            "unit": "%"
          },
          {
            "title": "Hb",
            "unit": "g/dL"
          },
          {
            "title": "Age",
            "unit": null
          }
        ],
        "result_obj": {
          "res_title": "Result",
          "res_default": "Please fill out the required fields.",
          "res_icon": "assets/icons/ic_info.svg"
        },
        "Ref_values_obj": {
          "title": "Reference Values:",
          "values": [
            {
              "val_title": "Formulae",
              "val_res": "Oxygen For below age 70 \noxygen/min= 125/min\n\nOxygen For above age 70 = oxygen/min= 110/min   \n\nBSA =   (height * weight) / 3600)^(1 / 2)\n\nFick’s Cardiac Output =  (oxygen/min * 100) * ( BSA / ((sao - svo) * hemoglobin * 13.4))\n\n Cardiac Index = Fick’s Cardiac Output/BSA"
            },   {
              "val_title": "SaO2",
              "val_res": "Normal range : 95-100"
            },
            {
              "val_title": "SvO2",
              "val_res": "Normal range : 60-80"
            },
            {
              "val_title": "Hemoglobin",
              "val_res": "Normal range : 12-17"
            }
          ]
        }
      },
      "Calculate Shock Risk Score": {
        "title": "Calculate Shock Risk Score",
        "exams": [
          {
            "title": "Physical Exam",
            "exam_parameters": [
